{
    "swagger": "2.0",
    "info": {
        "title": "Go Pro API",
        "description": "这是一个基于Golang开发的Web服务器",
        "version": "1.0.0",
        "termsOfService": "NO terms of service",
        "contact": {
            "email": "summng@qq.com"
        },
        "license": {
            "name": "The Apache License, Version 2.0",
            "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
        }
    },
    "basePath": "/v1",
    "paths": {
        "/account/login": {
            "post": {
                "tags": [
                    "account"
                ],
                "description": "手机/邮箱/昵称登录",
                "operationId": "AccountController.登录",
                "parameters": [
                    {
                        "in": "body",
                        "name": "body",
                        "description": "账号\u003cbr\u003e密码\u003cbr\u003e区号",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/.LoginParam"
                        }
                    }
                ]
            }
        },
        "/account/loginWithCode": {
            "post": {
                "tags": [
                    "account"
                ],
                "description": "手机验证码登录",
                "operationId": "AccountController.验证码登录",
                "parameters": [
                    {
                        "in": "body",
                        "name": "body",
                        "description": "手机账号\u003cbr\u003e验证码",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/.LoginWithCodeParam"
                        }
                    }
                ]
            }
        },
        "/account/sendAuthEmail": {
            "post": {
                "tags": [
                    "account"
                ],
                "description": "发送认证邮件和忘记密码",
                "operationId": "AccountController.发送验证邮件",
                "parameters": [
                    {
                        "in": "body",
                        "name": "body",
                        "description": "账号Id\u003cbr\u003e邮件地址\u003cbr\u003e邮件类型：1=邮箱认证，2=忘记密码",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/.SendAuthEmailParam"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "{string} {\"b\" : 1}"
                    }
                }
            }
        },
        "/account/sendEmail": {
            "post": {
                "tags": [
                    "account"
                ],
                "description": "发送Html邮件",
                "operationId": "AccountController.发送邮件",
                "parameters": [
                    {
                        "in": "body",
                        "name": "body",
                        "description": "收件人\u003cbr\u003e收件内容\u003cbr\u003e标题",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/.SendEmailParam"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "{string} {\"b\" : 1}"
                    }
                }
            }
        },
        "/account/sendSmsCode": {
            "post": {
                "tags": [
                    "account"
                ],
                "description": "发送短信验证码",
                "operationId": "AccountController.发送短信验证码",
                "parameters": [
                    {
                        "in": "body",
                        "name": "body",
                        "description": "手机号码",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/.SendSmsCodeParam"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "{string} {\"b\" : 1}"
                    }
                }
            }
        },
        "/stream/createStream/{url}": {
            "get": {
                "tags": [
                    "stream"
                ],
                "description": "创建一条相对时间的直播流",
                "operationId": "StreamController.创建直播流",
                "parameters": [
                    {
                        "in": "path",
                        "name": "url",
                        "description": "播放地址，请urlEncode",
                        "schema": {
                            "$ref": "#/definitions/.true"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "{string} {\"b\" : 1}"
                    }
                }
            }
        },
        "/stream/createSubtitle/{streamId}/{content}/{createTime}": {
            "get": {
                "tags": [
                    "stream"
                ],
                "description": "创建一条相对时间的直播流字幕",
                "operationId": "StreamController.创建直播流的字幕",
                "parameters": [
                    {
                        "in": "path",
                        "name": "streamId",
                        "description": "流id",
                        "schema": {
                            "$ref": "#/definitions/.true"
                        }
                    },
                    {
                        "in": "path",
                        "name": "content",
                        "description": "字幕内容，请urlEncode",
                        "schema": {
                            "$ref": "#/definitions/.true"
                        }
                    },
                    {
                        "in": "path",
                        "name": "createTime",
                        "description": "字幕出现时间，第几秒",
                        "schema": {
                            "$ref": "#/definitions/.true"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "{string} {\"b\" : 1}"
                    }
                }
            }
        },
        "/stream/getStream": {
            "get": {
                "tags": [
                    "stream"
                ],
                "description": "获取当前正在直播的流地址",
                "operationId": "StreamController.获取当前正在直播的流地址",
                "responses": {
                    "200": {
                        "description": "{string} {\"b\" : 1}"
                    }
                }
            }
        },
        "/stream/getSubtitles/{streamId}": {
            "get": {
                "tags": [
                    "stream"
                ],
                "description": "获取当前正在直播的流的所有字幕",
                "operationId": "StreamController.获取当前正在直播的流的所有字幕",
                "parameters": [
                    {
                        "in": "path",
                        "name": "streamId",
                        "description": "流id",
                        "schema": {
                            "$ref": "#/definitions/.true"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "{string} {\"b\" : 1}"
                    }
                }
            }
        }
    },
    "definitions": {
        ".LoginParam": {
            "type": "object"
        },
        ".LoginWithCodeParam": {
            "type": "object"
        },
        ".SendAuthEmailParam": {
            "type": "object"
        },
        ".SendEmailParam": {
            "type": "object"
        },
        ".SendSmsCodeParam": {
            "type": "object"
        },
        ".true": {
            "type": "object"
        }
    }
}